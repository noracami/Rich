<h1>Sign in</h1>

<%= form_with model: @user, url: sessions_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password, "密碼" %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "登入" %>
  </div>
<% end %>
